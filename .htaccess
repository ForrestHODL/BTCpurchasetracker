RewriteEngine On

# Redirect clean URLs to HTML files
RewriteRule ^home$ home.html [L]
RewriteRule ^btc-buy-tracker$ btc-buy-tracker.html [L]
RewriteRule ^treasury$ treasury.html [L]
RewriteRule ^press-release$ press-release.html [L]

# Redirect root to home
RewriteRule ^$ home.html [L]

# Optional: Redirect .html extension to clean URLs
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1? [NC,L,R=301]